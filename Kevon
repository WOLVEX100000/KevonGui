Tab1Section:NewButton("God Mode", "Say .god", function()

    game.Players.LocalPlayer.Chatted:connect(function(msg)

if string.lower(msg):sub(1) ==".god" then

	game.Players.LocalPlayer.Character.pvp:Destroy()	end

end)

end)

